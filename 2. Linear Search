from operator import index

list=[1,3,5,7,9,10,15]
s=int(input("Enter the Number you want to search:"))        # You can get value or you can declare here itself
def search(list,s):
    for i in list:
        if i==s:
            return True
    return False

if search(list,s):
    print("Number Found in index",index(list.index(s)+1))
else:
    print("Number not Found")
    
    
#Output:
    
Enter the Number you want to search:10
Number Found in index 6
